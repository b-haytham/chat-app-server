{
  "name": "chat-app-server",
  "description": "simple chat app ",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "b-haytham",
    "email": "boussarsarhaytham@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^12.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run compile && npm run mocha",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev --no-notify src/",
    "start": "npm run compile && node lib/",
    "mocha": "ts-mocha \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.9",
    "@feathersjs/authentication-local": "^4.5.9",
    "@feathersjs/authentication-oauth": "^4.5.9",
    "@feathersjs/configuration": "^4.5.9",
    "@feathersjs/errors": "^4.5.9",
    "@feathersjs/express": "^4.5.9",
    "@feathersjs/feathers": "^4.5.9",
    "@feathersjs/socketio": "^4.5.9",
    "@feathersjs/transport-commons": "^4.5.9",
    "@types/bcryptjs": "^2.4.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "feathers-hooks-common": "^5.0.4",
    "feathers-mongoose": "^8.3.1",
    "helmet": "^4.1.1",
    "mongodb-core": "^3.2.7",
    "mongoose": "^5.10.9",
    "serve-favicon": "^2.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/helmet": "0.0.48",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.0.3",
    "@types/mongoose": "^5.7.36",
    "@types/serve-favicon": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "axios": "^0.20.0",
    "eslint": "^7.11.0",
    "mocha": "^8.2.0",
    "shx": "^0.3.2",
    "ts-mocha": "^7.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
  }
}
